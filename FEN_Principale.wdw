        code : |1-
         
         // Variables
         sdExplain    est une Source de données
         sXML         est une chaine
         GR_DETAILS..visible = faux
         // Vérifie la saisie
         SI VerifieSaisie() = Vrai ALORS
         	
         	// Requête SQL ?
         	SI rtfverstexte(SAI_REQUETE_SQL) ~= "" ALORS
         		Erreur(<§$0005§>)
         		RepriseSaisie(SAI_REQUETE_SQL)	
         	FIN
         	
         	// Ouvre la connexion
         	OuvreConnexion()
         	
         	// Execute la requête SQL
         	si HExécuteRequêteSQL(sdExplain,"EXPLAIN "+RTFVersTexte(SAI_REQUETE_SQL)) = faux ALORS
         		erreur(herreurinfo())
         		reprisesaisie(SAI_REQUETE_SQL)
         	FIN
         	
         	// Lit le premier enregistrement pour récupérer le XML
         	HLitPremier(sdExplain)
         	sXML = sdExplain.EXPLAIN
         	
         	// Construit l'organigramme à partir du XML
         	AfficheOrganigramme(sXML)
         	clColorationXML est un cColorationXML(xmlverstexte(UTF8VersChaîne(sXML)))
         	clColorationXML.SetModeSortie(cColorationXML.OUt_RTF)
         	
         	SAI_XML = clColorationXML.Coloration()
         FIN
         
         CAS EXCEPTION:
         	Erreur(ExceptionInfo())
         FIN:
         	// Ferme la connexion
         	FermeConnexion()
