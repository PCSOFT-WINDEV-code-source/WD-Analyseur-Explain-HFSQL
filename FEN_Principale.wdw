      SI SAI_CHEMIN_FICHIER ~= "" OU fRépertoireExiste(SAI_CHEMIN_FICHIER) = Faux ALORS
      	Erreur(<§$0007§>)
      	RepriseSaisie(SAI_CHEMIN_FICHIER)
      FIN
      
      RENVOYER Vrai
