   -
     name : VerifieSaisieClientServeur
     procedure_id : 1197056760341028712
     type_code : 14
     code : |1-
      // Résumé : Vérifie la saisie des champs HFSQL Client/Serveur
      // Syntaxe :
      //[ <Résultat> = ] VerifieSaisieClientServeur ( [<bVerifieBDD> est booléen])
      //
      // Paramètres :
      // 	bVerifieBDD (booléen - valeur par défaut=1) : Vrai si on doit vérifier la base de données
      // Valeur de retour :
      // 	booléen : Vrai si les données sont correctes
      //
      PROCEDURE VerifieSaisieClientServeur(LOCAL bVerifieBDD est un booléen = Vrai)
      
      SI SAI_SERVEUR ~= "" ALORS
      	Erreur(<§$0000§>)
      	RepriseSaisie(SAI_SERVEUR)	
      FIN
      
      SI Val(SAI_PORT) = 0 ALORS
      	Erreur(<§$0001§>)
      	RepriseSaisie(SAI_PORT)
      FIN
      
      SI SAI_UTILISATEUR ~= "" ALORS
      	Erreur(<§$0002§>)
      	RepriseSaisie(SAI_UTILISATEUR)
      FIN
      
      SI bVerifieBDD ALORS
      	SI SAI_BDD ~= "" ALORS
      		Erreur(<§$0003§>)
      		RepriseSaisie(SAI_BDD)
      	FIN
      FIN
      
      RENVOYER Vrai
