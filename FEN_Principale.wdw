      // Résumé : Ouvre la connexion vers les fichiers HF Classic
      // Syntaxe :
      //[ <Résultat> = ] _OuvreConnexionClassic ()
      //// Traitement automatique des erreurs :   exécuter le bloc de code CAS ERREUR
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE PRIVE _OuvreConnexionClassic()
      
      si HOuvreAnalyse(SAI_CHEMIN_ANALYSE, SAI_MOTDEPASSE_CLASSIC) = faux ALORS
      	erreur("Impossible d'ouvrir l'analyse.","Détails : " ,herreurinfo())
      	renvoyer faux
      FIN
      
      HPasse("*",SAI_MOTDEPASSE_FICHIERHF_CLASSIC)
      
      HChangeRep("*",ComplèteRep(SAI_CHEMIN_FICHIER))
      
      renvoyer vrai
      
      CAS ERREUR:
      	ErreurPropage()
      CAS EXCEPTION:
      	ExceptionPropage()
