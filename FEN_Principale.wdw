   -
     name : _AfficheNoeud
     internal_properties : 
     procedure_id : 1197071307397270603
     type_code : 14
     code : |1+
      // Résumé : Affiche un noeud du XML de EXPLAIN
      // Syntaxe :
      //_AfficheNoeud (<xmlExplainNoeud> est xmlNoeud [, <nIndiceParent> est entier])
      //
      // Paramètres :
      //	xmlExplainNoeud (xmlNoeud) : Noeud du XML de EXPLAIN
      //	nIndiceParent (entier - valeur par défaut=-1) : L'indice de l'élément parent
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PRIVE _AfficheNoeud(xmlExplainNoeud est un xmlNoeud, nIndiceParent est un entier = -1)
      
      nNouvelIndice est un entier
      sType         est une chaine = xmlExplainNoeud..Attribut["type"]..Valeur
      xmlNoeudFils  est un xmlNoeud
      sNom          est une chaine
      tmpElement    est un OrgaElément
      bStoppe est un booléen
      
      si pas sType dans ("materialize","aggregate") ALORS
      	SI nIndiceParent = -1 ALORS
      		nNouvelIndice = OrgaAjoute(ORGA_EXPLAIN,sType)
      	SINON
      		nNouvelIndice = OrgaAjouteFils(ORGA_EXPLAIN,nIndiceParent,sType)	
      	FIN
      FIN
      
      si nNouvelIndice > 0 ALORS
      	// Stocke le type
      	tmpElement.Note = sType
      	tmpElement.Couleurfond = blanc
      	
      	// Selon le type de plan, il a des infos à afficher
      	SELON sType
      		cas "table"
      			tmpElement.Titre +=" " + xmlExplainNoeud.name..Texte
      			si xmlExplainNoeud."index-scan"..Attribut["key"] <> "" ALORS
      				tmpElement.Contenu = "Clé utilisée : " + xmlExplainNoeud."index-scan"..Attribut["key"]
      			FIN
      			si xmlExplainNoeud.filter..Texte = "" ALORS
      				tmpElement.Contenu += [RC] + "Parcours complet du fichier"
      				tmpElement.CouleurFond = RougePastel
      			sinon							
      				tmpElement.Contenu += [RC] + "Filtre : " + xmlExplainNoeud.filter..Texte
      			FIN
      			bStoppe = Vrai
      		CAS "join"
      			tmpElement.Titre = "Jointure"
      			tmpElement.Contenu = "Méthode : " + xmlExplainNoeud."join-method"..Texte 
      			tmpElement.Contenu += [RC] + "Type : " +  xmlExplainNoeud."join-type"..Texte	
      			
      		CAS "union"
      			// Rien à faire
      			tmpElement.Titre = "Union"
      		CAS "materialize"
      			// Rien à faire
      			
      		CAS "sort"
      			tmpElement.Titre = "Tri"
      			tmpElement.Contenu = "Rubrique(s) triée(s) : " 
      			sRubriques est une chaine
      			
      			pour i = 1 _a_ xmlExplainNoeud."sort-key"..NoeudFils..Occurrence
      				sRubriques += [", "] + xmlExplainNoeud."sort-key"..NoeudFils[i]..Texte
      			FIN
      			
      			tmpElement.Contenu += sRubriques
      			
      		CAS "distinct"
      			// Rien à faire
      			
      		CAS "limit"
      			
      			tmpElement.Titre = "Limite"
      			
      			tmpElement.Contenu = "Type : " + xmlExplainNoeud.type..Texte
      			tmpElement.Contenu += [RC] + "Nb Lignes : " + xmlExplainNoeud.rows..Texte		
      			tmpElement.Contenu += [RC] + "offset = " + val(xmlExplainNoeud.offset..Texte)
      			si xmlExplainNoeud."rows-percent"..Texte = "true" ALORS
      				tmpElement.contenu += [RC] + "Pourcentage : Oui"
      			sinon
      				tmpElement.contenu += [RC] + "Pourcentage : Non"				
      			FIN
      						
      		CAS "filter"
      			tmpElement.Contenu = xmlExplainNoeud.filter..Texte
      			
      			
      		AUTRE CAS:
      			dbgAssertion(Faux,"Type de plan non géré")
      	FIN
      	
      	// Met à jour l'élément ajouté
      	ORGA_EXPLAIN[nNouvelIndice] =  tmpElement
      sinon
      	nNouvelIndice = nIndiceParent
      FIN
      
      si bStoppe = vrai ALORS
      	retour
      FIN
      
      // On continue de parcourir le fils en ajoutant les plans fils
      POUR TOUT xmlNoeudFils DE xmlExplainNoeud..NoeudFils
      	sNom = xmlNoeudFils..Nom
      	SI sNom = "plan" ALORS		
      		_AfficheNoeud(xmlNoeudFils,nNouvelIndice)	
      	FIN
      FIN
      
