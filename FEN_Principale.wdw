      // Résumé : Ouvre la connexion Client/Serveur
      // Syntaxe :
      //_OuvreConnexionClientServeur ()
      
      PROCEDURE PRIVE _OuvreConnexionClientServeur()
      
      // Variables
      sListeFichier est une chaine
      sNomFichier   est une chaine
      taFichiers    est un tableau associatif de chaines
      
      // Description de la connexion 
      gcnxConnexion..Utilisateur    = SAI_UTILISATEUR 
      gcnxConnexion..MotDePasse     = SAI_MOTDEPASSE_SERVEUR 
      gcnxConnexion..Serveur        = SAI_SERVEUR+":"+SAI_PORT 
      gcnxConnexion..BaseDeDonnées  = SAI_BDD 
      gcnxConnexion..Provider       = hAccèsHFClientServeur 
      gcnxConnexion..Accès          = hOLectureEcriture 
      gcnxConnexion..OptionsCurseur = hCurseurClient 
      
      // Ouvre la connexion au serveur 
      HOuvreConnexion(gcnxConnexion)
      
      // Liste les fichiers
      sListeFichier = HListeFichier(gcnxConnexion)
      
      // Déclare les fichiers
      POUR TOUTE CHAINE sFichier de sListeFichier SEPAREE PAR RC
      	
      	// Extrait le nom du fichier
      	sNomFichier = fExtraitChemin(sFichier,fFichier)
      	
      	// Déjà déclaré ?
      	SI taFichiers[sNomFichier]..Vide ALORS
      		// Déclare le fichier pour pouvoir exécuter la requête SQL
      		HPasse(sNomFichier,SAI_MOTDEPASSE_FICHIERHF_SERVEUR)
      		si HDéclareExterne(sFichier,sNomFichier,gcnxConnexion) = faux ALORS
      			HErreurMotDePasse()
      			continuer
      		FIN
      		
      		taFichiers[sNomFichier] = sNomFichier
      	FIN	
      FIN
      
      CAS ERREUR:
      	ErreurPropage()
      CAS EXCEPTION:
      	ExceptionPropage()
